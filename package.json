{
  "name": "portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:all": "concurrently \"npm run dev\" \"npm run email:dev\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev:wrangler": "wrangler dev --env dev",
    "dev:all:wrangler": "concurrently \"npm run dev:wrangler\" \"npm run dev\"",
    "email:dev": "email dev --dir src/assets",
    "email:build": "email build --dir src/assets",
    "deploy:wrangler": "wrangler deploy --env production"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "devicon": "^2.17.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-email": "^4.2.12",
    "resend": "^6.1.2",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.10",
    "@eslint/js": "^9.35.0",
    "@react-email/components": "0.5.5",
    "@react-email/preview-server": "^4.2.12",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "concurrently": "^9.2.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.9",
    "wrangler": "^4.42.0"
  }
}
