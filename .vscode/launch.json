{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Start Vite Dev Server î£™",
      "program": "${workspaceFolder}/node_modules/vite/bin/vite.js",
      "args": [
        "--mode",
        "development"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "env": {
        "NODE_ENV": "development"
      },
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}"
    }
  ]
}
